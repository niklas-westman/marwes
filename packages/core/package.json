{
  "name": "@marwes-ui/core",
  "version": "0.0.4",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:typecheck": "tsc -p tsconfig.test.json --noEmit",
    "test": "vitest run",
    "clean": "rimraf dist",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "generate:icons": "tsx scripts/generate-icon-names.ts"
  },
  "devDependencies": {
    "rimraf": "^6.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  }
}
